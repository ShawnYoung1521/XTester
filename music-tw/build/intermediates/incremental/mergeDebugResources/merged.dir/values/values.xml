    <declare-styleable name="CircleImageView">
        <attr format="dimension" name="border_width"/>
        <attr format="color" name="border_color"/>
        <attr format="boolean" name="border_overlay"/>
    </declare-styleable>
    <declare-styleable name="CustomAudioIcon">
        <attr name="type">
            <enum name="start" value="0"/>            
            <enum name="forward" value="1"/>
            <enum name="backward" value="2"/>
            <enum name="exit" value="3"/>
            <enum name="mode" value="4"/>
        </attr>
        <attr format="color" name="mycolor"/>
    </declare-styleable>
    <declare-styleable name="HorizontalListView">
        <attr format="dimension" name="dividerWidth"/>
        <attr name="android:divider"/>
        <attr name="android:requiresFadingEdge"/>
        <attr name="android:fadingEdgeLength"/>
    </declare-styleable>
    <declare-styleable name="Knob">
        <attr format="string" name="label"/>
        <attr format="integer" name="background1"/>
        <attr format="integer" name="foreground"/>
    </declare-styleable>
    <declare-styleable name="RoundImageView">  
        <attr format="dimension" name="borderRadius"/>  
	    <attr name="img_type">  
	        <enum name="circle" value="0"/>  
	        <enum name="round" value="1"/>  
	    </attr>  
    </declare-styleable>
    <declare-styleable name="SmoothCheckBox">
        <attr format="integer" name="duration"/>
        <attr format="dimension" name="stroke_width"/>
        <attr format="color" name="color_tick"/>
        <attr format="color" name="color_checked"/>
        <attr format="color" name="color_unchecked"/>
        <attr format="color" name="color_unchecked_stroke"/>
    </declare-styleable>
